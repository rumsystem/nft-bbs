{
   "name": "nft-bbs-server",
   "version": "1.0.0",
   "description": "",
   "main": "dist/index.js",
   "types": "dist/index.d.ts",
   "scripts": {
      "dev": "cross-env NODE_ENV=development nodemon --config ./nodemon.json",
      "dev:debug": "cross-env NODE_ENV=development nodemon --config ./nodemon-debug.json",
      "build": "rimraf dist && yarn tsc -b",
      "start": "ts-node-transpile-only index.ts",
      "start:prod": "node dist/index.js",
      "lint": "eslint --ext \"js,jsx,ts,tsx\" .",
      "typeorm": "node -r tsconfig-paths/register ../../node_modules/typeorm/cli-ts-node-commonjs.js -d ./orm/data-source.ts",
      "reset": "node -r tsconfig-paths/register ../../node_modules/typeorm/cli-ts-node-commonjs.js -d ./orm/data-source.ts schema:drop && ts-node-transpile-only utils/reset.ts",
      "migration": "node -r tsconfig-paths/register ../../node_modules/typeorm/cli-ts-node-commonjs.js -d ./orm/data-source.ts migration:generate"
   },
   "keywords": [],
   "author": "raimonfuns",
   "license": "ISC",
   "dependencies": {
      "@fastify/cors": "^8.1.0",
      "date-fns": "^2.29.3",
      "dotenv": "^16.0.3",
      "fastify": "^4.8.1",
      "fp-ts": "^2.12.2",
      "http-errors": "^2.0.0",
      "io-ts": "^2.2.19",
      "js-yaml": "^4.1.0",
      "lodash": "^4.17.21",
      "lru-cache": "^7.14.0",
      "module-alias": "^2.2.2",
      "nft-bbs-types": "*",
      "node-localstorage": "^2.2.1",
      "nodemon": "^2.0.20",
      "pg": "^8.4.0",
      "quorum-light-node-sdk-nodejs": "^0.1.31",
      "reflect-metadata": "^0.1.13",
      "socket.io": "^4.5.2",
      "typeorm": "0.3.10"
   },
   "devDependencies": {
      "@types/http-errors": "^1.8.2",
      "@types/js-yaml": "^4.0.5",
      "@types/lodash": "^4.14.186",
      "@types/module-alias": "^2.0.1",
      "@types/node": "^18.8.5",
      "rimraf": "^3.0.2",
      "ts-node": "10.9.1",
      "tsconfig-paths": "^4.1.0"
   }
}
