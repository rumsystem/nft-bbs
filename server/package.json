{
   "name": "nft-bbs-server",
   "version": "1.0.0",
   "description": "",
   "main": "dist/index.js",
   "types": "dist/index.d.ts",
   "scripts": {
      "dev": "nodemon --config ./nodemon.json",
      "dev:debug": "nodemon --config ./nodemon-debug.json",
      "build": "yarn tsc",
      "start": "ts-node-transpile-only index.ts",
      "start:prod": "node dist/index.js",
      "typeorm": "node -r tsconfig-paths/register node_modules/typeorm/cli-ts-node-commonjs.js -d ./orm/data-source.ts",
      "reset": "node -r tsconfig-paths/register node_modules/typeorm/cli-ts-node-commonjs.js -d ./orm/data-source.ts schema:drop && ts-node-transpile-only utils/reset.ts",
      "migration": "node -r tsconfig-paths/register node_modules/typeorm/cli-ts-node-commonjs.js -d ./orm/data-source.ts migration:generate"
   },
   "keywords": [],
   "author": "raimonfuns",
   "license": "ISC",
   "dependencies": {
      "@fastify/cors": "^8.1.0",
      "date-fns": "^2.29.2",
      "fastify": "^4.5.3",
      "fp-ts": "^2.12.2",
      "http-errors": "^2.0.0",
      "io-ts": "^2.2.17",
      "lodash": "^4.17.21",
      "lru-cache": "^7.14.0",
      "module-alias": "^2.2.2",
      "node-localstorage": "^2.2.1",
      "nodemon": "^2.0.19",
      "pg": "^8.4.0",
      "quorum-light-node-sdk-nodejs": "^0.1.20",
      "reflect-metadata": "^0.1.13",
      "socket.io": "^4.5.1",
      "typeorm": "0.3.7",
      "typescript": "^4.7.4"
   },
   "devDependencies": {
      "@types/http-errors": "^1.8.2",
      "@types/lodash": "^4.14.184",
      "@types/module-alias": "^2.0.1",
      "@types/node": "^16.11.10",
      "ts-node": "10.7.0",
      "tsconfig-paths": "^4.1.0"
   }
}
